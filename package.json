{
  "name": "xport-tableau-extension",
  "version": "0.1.0",
  "description": "Xport Data",
  "scripts": {
    "start": "node node_modules/http-server/bin/http-server -p "process.env.PORT,
    "lint": "semistandard"
  },
  "author": "xpandit",
  "dependencies": {
    "http-server": "^0.11.1"
  },
  "devDependencies": {
    "semistandard": "^11.0.0"
  },
  "semistandard": {
    "env": {
      "jquery": true,
      "browser": true,
      "tableau": true
    },
    "globals": [
      "tableau"
    ],
    "ignore": [
      "/lib",
      "/docs"
    ]
  }
}
